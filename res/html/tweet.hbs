                    </div>
                </div>
                {{#if reply_to}}
                <div class="reply-to dim">
                    Replying to <span class="twitter-link">@{{reply_to}}</span>
                </div>
                {{/if}}
                <div class="tweet-text bright">
                    <span>{{{tweet_text}}}</span>
                </div>
                {{#if image}}
                <div class="img-container">
                    <img class="tweet-image" src="{{image}}">
                </div>
                {{/if}}
                {{#if fact_check}}
