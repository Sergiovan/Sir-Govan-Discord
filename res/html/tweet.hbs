                    </div>
                </div>
            </div>
            {{#if more_tweets}}
            <div class="separator"></div>
            <div class="more-replies bright">
                More tweets
            </div>
            {{#each more_tweets}}
            <div class="reply-tweet">
                <div class="reply-container default-props">
                    <div class="avatar">
                        <img src="{{avatar}}" title="" width="49" height="49">
                    </div>
                    <div class="right-side-reply">
                        <div class="main-user-info">
                            <span class="name-container">
                                <span class="name bright">{{name}}</span>
                                {{#if verified}}
                                <span class="verified" >
